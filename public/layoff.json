import json

# Load the existing data from layoff.json
with open('layoff.json', 'r') as file:
    data = json.load(file)

# Iterate through each entry in the JSON and add the "Job_type": "" key-value pair
for entry in data: # Assuming data is a list of dictionaries
    entry["Job_type"] = ""  # Correct way to add a key-value pair

# Save the modified data back to layoff.json
with open('layoff.json', 'w') as file:
    json.dump(data, file, indent=4)